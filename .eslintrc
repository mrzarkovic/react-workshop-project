{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "react/jsx-boolean-value": 1,
    "react/prefer-es6-class": 1
  },
  "extends": [
    "prettier",
    "prettier/react"
  ]
}